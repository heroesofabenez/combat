{
    "name": "heroesofabenez/combat",
    "description": "Combat library for PHP applications",
    "keywords": [
      "combat"
    ],
    "license": "BSD-3-Clause",
    "homepage": "https://heroesofabenez.gitlab.io",
    "authors": [
        {
            "name": "Jakub Konečný",
            "email": "jakub.konecny2@centrum.cz",
            "role": "Developer"
        }
    ],
    "support": {
        "source": "https://gitlab.com/heroesofabenez/combat/",
        "issues": "https://gitlab.com/heroesofabenez/combat/issues",
        "docs": "https://heroesofabenez.gitlab.io/combat"
    },
    "require": {
        "php": ">=7.2.0",
        "latte/latte": "^2.5 || ^3.0",
        "nette/utils": "^2.5 || ^3.0",
        "nexendrie/translation": "^1.1.1 || ^2.0.1",
        "nexendrie/utils": "^1.6.1",
        "symfony/options-resolver": "^4.0 || ^5.0"
    },
    "require-dev": {
        "nexendrie/code-quality": "dev-master",
        "nexendrie/site-generator": "^0.6.2",
        "phpstan/phpstan-nette": "^0.12",
        "nette/http": "^2.4.7 || ^3.0",
        "nette/di": "^2.4.10 || ^3.0"
    },
    "autoload": {
        "psr-4": {
            "HeroesofAbenez\\Combat\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "HeroesofAbenez\\Combat\\": "tests/HeroesofAbenez/Combat/"
        }
    },
    "config": {
        "classmap-authoritative": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
