{
    "name": "heroesofabenez/combat",
    "description": "Combat library for PHP applications",
    "keywords": [
      "combat"
    ],
    "license": "BSD-3-Clause",
    "homepage": "https://heroesofabenez.gitlab.io",
    "authors": [
        {
            "name": "Jakub Konečný",
            "email": "jakub.konecny2@centrum.cz",
            "role": "Developer"
        }
    ],
    "support": {
        "source": "https://gitlab.com/heroesofabenez/combat/",
        "issues": "https://gitlab.com/heroesofabenez/combat/-/issues",
        "docs": "https://heroesofabenez.gitlab.io/combat"
    },
    "require": {
        "php": ">=8.1.0",
        "latte/latte": "^2.8",
        "nette/utils": "^3.2.2",
        "nexendrie/translation": "^2.3.1",
        "nexendrie/utils": "^1.8",
        "symfony/options-resolver": "^5.1"
    },
    "require-dev": {
        "nexendrie/code-quality": "dev-master",
        "nexendrie/site-generator": "^0.7",
        "phpstan/phpstan-nette": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "HeroesofAbenez\\Combat\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "HeroesofAbenez\\Combat\\": "tests/HeroesofAbenez/Combat/"
        }
    },
    "config": {
        "classmap-authoritative": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
